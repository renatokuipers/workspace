{
  "name": "openmanus",
  "version": "1.0.0",
  "description": "Local AI assistant platform combining Node.js backend with a React frontend",
  "main": "server/server.js",
  "scripts": {
    "client:start": "cd client && npm run dev",
    "server:start": "cd server && node server.js",
    "start": "concurrently -n \"client,server\" \"npm run client:start\" \"npm run server:start\"",
    "client:dev": "cd client && npm start",
    "server:dev": "cd server && node server.js",
    "dev": "npm run start",
    "client:build": "cd client && npm run build",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "ai",
    "assistant",
    "local",
    "node",
    "react",
    "python"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}